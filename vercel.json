{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept"
        }
      ]
    }
  ],
  "routes": [{ "src": "/api/calculate.ts", "dest": "/src/api/nested-api.ts" }],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
